{
    "version": 2,
    "builds": [
        {
            "src": "build.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "staticfiles"
            }
        },
        {
            "src": "cesa_project/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "headers": {
                "Cache-Control": "s-maxage=31536000, immutable"
            },
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "cesa_project/wsgi.py"
        }
    ]
}