{
  "version": 2,
  "builds": [
    {
      "src": "cesa_project/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "buildCommand": "bash builds.sh",
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "cesa_project/wsgi.py"
    }
  ]
}
