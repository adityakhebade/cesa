{
    "version": 2,
    "builds": [
      {
        "src": "cesa_project/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.12"
        }
      },
      {
        "src": "public",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/public/$1"
      },
      {
          "src": "/media/(.*)",
          "dest": "/media/$1"
      },
      {
        "src": "/(.*)",
        "dest": "cesa_project/wsgi.py"
      }
    ]
  }
