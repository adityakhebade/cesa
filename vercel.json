{
    "builds": [
      { "src": "cesa_project/wsgi.py", "use": "@vercel/python", "config": { ... } },
      { "src": "build.sh", "use": "@vercel/static-build", "config": { "distDir": "staticfiles" } }
    ],
    "routes": [
      { "src": "/static/(.*)", "dest": "/static/$1" },
      { "src": "/(.*)", "dest": "cesa_project/wsgi.py" }
    ]
  }
  